# Strongswan Connection Secrets File: <% @title %>
#
# * Managed by Puppet, do not modify *
#
<% @secrets.each do |v|
     if ['RSA', 'ECDSA'].include?(v['auth']) -%>
: <%= v['auth'] %> <%= v['key'] %><% if v.key?('passphrase') %> "<%= v['passphrase'] %>"<% end %>
<%   else -%>
<%= v['left_id'] %> <%= v['right_id'] %> : <%= v['auth'] %> "<%= v['key'] %>"
<%   end
   end -%>
